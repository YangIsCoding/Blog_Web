generator client {
  provider = "prisma-client-js"
}

datasource db {
  //provider = "sqlite"
  provider = "mysql"
  url      = env("DATABASE_URL")
 //url = "file:./dev.db"
}

model ContactForm {
  id          Int      @id @default(autoincrement())
  name        String
  email       String
  phoneNumber String?
  message     String
  createdAt   DateTime @default(now())
}
